<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.uw.nmcgov.recommendme">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_GPS" />
    <uses-permission android:name="android.permission.ACCESS_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ALLOW_MOCK_LOCATIONS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".HomePageActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RecommendationsForYou" />
        <activity android:name=".SearchForRecommendations" />
        <activity android:name=".RecommendationSearchResults" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".CreateProfileActivity" />
        <activity android:name=".ExistingLogin" />
        <activity android:name=".CircleGraphicActivity" />
        <activity android:name=".AccountsActivity" />
        <activity android:name=".MediaDetails" />
        <activity android:name=".SavedActivity" />
        <activity android:name=".ManageAccountActivity" />
        <activity android:name=".UpdateEmailActivity" />
        <activity android:name=".CoolTipActivity" />
        <activity android:name=".SetupPageActivity" />
        <!--
 ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information.
        -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

    </application>

</manifest>